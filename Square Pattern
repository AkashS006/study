                 // PATERN PRINTING PROGRAM
#include <stdio.h>
int main(){
    int n;
    printf("Enter the number rows to print pattern : ");
// inner loop is using for iterations
// outer loop is using for number of rows
    scanf("%d",&n);
    for(int i=0;i<n;i++){
        for(int j =0; j<=5; j++){
            printf("* ");
        }
        printf("\n");
    }
}
